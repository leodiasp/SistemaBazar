var
  FrmFornecedor: TFrmFornecedor;

implementation

uses DmModuloIII, UntMenuPrincipal;

{$R *.dfm}

procedure TFrmFornecedor.btnIncluirClick(Sender: TObject);
begin
  inherited;
   DBEdit2.SetFocus;
end;

procedure TFrmFornecedor.btnPesquisarClick(Sender: TObject);
begin
  FrmMenuPrincipal.idprocessamento := 'Fornecedor';
  inherited;

end;

procedure TFrmFornecedor.DBEdit2KeyDown(Sender: TObject; var Key: Word;
  Shift: TShiftState);
begin
  inherited;
if Key = VK_RETURN then Perform(Wm_NextDlgCtl,0,0);
end;

